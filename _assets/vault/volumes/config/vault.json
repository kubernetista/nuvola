{
    "storage": {
        "file": {
            "path": "/vault/file"
        }
    },
    "listener": {
        "tcp": {
            "address": "0.0.0.0:8200",
            "tls_disable": 0,
            "tls_cert_file": "/vault/tls-certs/127.0.0.1.pem",
            "tls_key_file": "/vault/tls-certs/127.0.0.1-key.pem"
        }
    },
    "api_addr": "http://127.0.0.1:8200",
    "ui": true,
    "storage_configuration": {
        "secret/": {
            "type": "kv-v2"
        }
    },
    "log_level": "trace",
    "log_format": "standard",
    "log_file": "/vault/logs/vault.log"
}